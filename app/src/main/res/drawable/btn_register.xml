<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 圆角 -->
    <corners
        android:radius="9dp" /><!-- 设置圆角半径 -->

    <!-- 大小 -->
    <size
        android:width="150dp"
        android:height="30dp"/><!-- 宽度和高度 -->

    <!-- 填充 -->
    <solid
        android:color="#ffffff"/><!-- 填充的颜色 -->

    <!--边框-->
    <stroke
        android:width="0.5dp"
        android:color="#66000000"/>

</shape>